// Criando um reducer que aceita actions de tipos distintos nÃ£o foi atualizado o estado
const defaultState = {
  authenticated: false
};
const LOGIN = 'LOGIN';
const LOGOUT = 'LOGOUT';

const authReducer = (state = defaultState, action) => {
  // Change code below this line
  switch(action.type) {
    case LOGIN:
    return { authenticated: true, };
      case LOGOUT:
    return { autehnticated: false, };
    default: 
    return defaulState;
  }
  
  // Change code above this line

};

const store = Redux.createStore(authReducer);

const loginUser = () => {
  return {
    type: 'LOGIN'
  }
};

const logoutUser = () => {
  return {
    type: 'LOGOUT'
  }
};